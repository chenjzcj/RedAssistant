<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="1.0px" android:color="@color/_00cac8"/>
            <corners android:radius="10dp"/>
            <gradient android:angle="270.0" android:endColor="@color/_00cac8" android:startColor="@color/_00cac8"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0px" android:color="@color/_2de1df"/>
            <corners android:radius="10dp"/>
            <gradient android:angle="270.0" android:endColor="@color/_2de1df" android:startColor="@color/_2de1df"/>
        </shape>
    </item>

</selector>